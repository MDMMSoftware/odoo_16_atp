<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="requisition_form_inherit_part_id" model="ir.ui.view">
            <field name="name">requisition.form.inherit</field>
            <field name="model">requisition</field> 
            <field name="inherit_id" ref="requisition.requisition_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet//notebook//page//field[@name='requisition_line']/tree/field[@name='product_desc']" position="after">
                    <field name="custom_part" readonly="1"/>
                </xpath> 						
            </field>
        </record>   
    </data>
</odoo>