<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="0">
      
        <!-- Group Rule -->

        <record model="ir.module.category" id="module_category_gps_tracker">
            <field name="name">GPS Tracker</field>
        </record>

        <record id="group_gps_tracker_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="gps_tracker.module_category_gps_tracker"/>
        </record>

        <record id="group_gps_tracker_data_entry" model="res.groups">
            <field name="name">Data Entry</field>
            <field name="implied_ids" eval="[(4, ref('gps_tracker.group_gps_tracker_user'))]"/>
            <field name="category_id" ref="gps_tracker.module_category_gps_tracker"/>
        </record>

    <record id="supergroup_gps_tracker" model="res.groups">
        <field name="name">Super Access to GPS Tracker</field>
    </record>

    <delete id="supergroup_gps_tracker" model="res.groups"/>
    
</data>
</odoo>
