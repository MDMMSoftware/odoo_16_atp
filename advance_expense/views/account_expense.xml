        <record id="view_direct_exp_wizard" model="ir.ui.view">
            <field name="name">Direct Expense Wizard</field>
            <field name="model">direct.exp.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="date"/>
                                <field name="amount"/>
                            </group>
                            <group>
                                <field name="account_id"/>
                                <field name="division_id" options="{'no_create':True,'no_create_edit':True}"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_dir_exp_payment" string="Payment" type="object" class="btn-primary" data-hotkey="q"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z" />
                    </footer>
                </form>
            </field>
        </record>

