            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Create a journal entry
              </p><p>
                A journal entry consists of several journal items, each of
                which is either a debit or a credit transaction.
              </p>
            </field>
        </record>    

        <record id="view_job_code" model="ir.ui.view">
            <field name="name">job.code.tree</field>
            <field name="model">job.code</field>
            <field name="arch" type="xml">
                <tree string="Job Code">
                    <field name="display_name" string="Job Code" />
                    <field name="description" string="Description" />
                    <field name="parent_id"/>
                </tree>
            </field>
        </record>

        <record id="view_job_code_form" model="ir.ui.view">
            <field name="name">job.code.form</field>
            <field name="model">job.code</field>
            <field name="arch" type="xml">
                <form string="Job Code">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" string="Job Code"/>
                            <h1><field name="name" placeholder="e.g. Something ..."/></h1>
                        </div>
                        <group>
                            <group name="left" col="2">
                                <field name="parent_id" string="Parent Job Code" class="oe_inline" options="{'no_quick_create': true, 'no_create_edit':true}" />
                                <field name="company_id" string="Company" class="oe_inline" options="{'no_quick_create': true, 'no_create_edit':true}" />
                            </group>
                            <group name="right">   
                                <field name="description" widget="text"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_job_code" model="ir.actions.act_window">
            <field name="name">Job Code</field>
            <field name="res_model">job.code</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                Create a job code
                </p><p>
                A job code is an entity of journal items.
                </p>            
            </field>
        </record>        

        <record id="view_report_remark" model="ir.ui.view">
            <field name="name">report.remark.tree</field>
            <field name="model">report.remark</field>
            <field name="arch" type="xml">
                <tree string="Report Remark" editable="top">
                    <field name="name"/>
                    <field name="company_id"/>
                </tree>
            </field>
        </record>

        <record id="action_report_remark" model="ir.actions.act_window">
            <field name="name">Report Remark</field>
            <field name="res_model">report.remark</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_report_remark"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
